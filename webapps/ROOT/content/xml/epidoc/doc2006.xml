                        <rs
                            key="#Landed_possessions #Property_of_persons_without_heirs #Fiscal_property #Textual_occurrence #Object #Inflow #Inheritance #Outflow #Allocation_to_churches"
                            >cuncta</rs> quae <persName
                            key="#Gastaldus/vicegastaldus #Landed_possessions #Property_of_persons_without_heirs"
                            ref="#people/116">Poto</persName>
                        <rs key="#Gastaldus/vicegastaldus">gastaldeus</rs> filius supradicti
                                <persName><name ref="Poto">Potonis</name></persName> possidere
                        videbatur in pertinentiis <placeName ref="#places/112"
                            key="#City_territory #Landed_possessions #Property_of_persons_without_heirs #Fiscal_property #Textual_occurrence #Object #Inflow #Inheritance #Outflow #Allocation_to_church"
