            <text>
                <body>
                    <div type="edition">
                        <div type="textpart">
                            <ab><persName key="#Vir_devotus/vir_honestus"><name ref="Ermithancus"
                                >Emithancu</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">vir honestus</rs> filius <persName><name
                                ref="Emmus">Emmi</name></persName> una cum coiuge sua <persName
                            key="#Vir_devotus/vir_honestus">Teuflada</persName> filia quondam
                                <persName><name ref="Muccius">Muccioni</name></persName>
                        <foreign>vendono a</foreign>
                        <persName key="#Priests"><name ref="Tanualdus">Tanualdi</name></persName>
                        viro venerabili <rs key="#Priests">presbitero</rs> de <orgName
                            key="#Churches" ref="#juridical_persons/243">beatus Sancto
                            Reculi</orgName> medietate de <rs key="#Casae/cassinae_massaricie"
                            >case</rs> me in <placeName ref="#places/672"
                            key="#Casae/cassinae_massaricie #Landed_possessions #Cereals #Wine #Fruits/vegetables #Acorns"
                            >Paternu</placeName>, <rs key="#Casae/cassinae_massaricie">case</rs>
                        <persName key="#Casae/cassinae_massaricie"><name ref="Minculus"
                                >Minculi</name></persName> et <persName
                            key="#Casae/cassinae_massaricie"><name ref="Raccorus"
                            >Racculi</name></persName> cum <rs key="#Landed_possessions">terra</rs>
                        et <rs key="#Wine">vineas</rs>, <rs key="#Cereals">campis</rs>, <rs
                            key="#Acorns">silvis</rs>, cum <rs key="#Fruits/vegetables">arbustra
                            fructefera,</rs> cum fontis; omnia in adpretiato in tregenta soledus (…)
                        quem tertio homo Deus timente dixere. <foreign>Prezzo</foreign>: auris
                        soledus nomero tregenta. <foreign>Pena in duplum. Scrive</foreign>:
                            <persName key="#Writer"><name ref="Albertus">Alipertu</name></persName>
                        amico meo. (…) Actum ad <rs key="#Monasteries">monstio</rs>
                        <orgName key="#Monasteries">Sanctu Donatu</orgName> in <placeName
                            key="#Monasteries">Fasqua</placeName>, in <rs key="#Fines">finibus</rs>
                        <geogName key="#Fines" ref="#estates/107">Voloterrana</geogName>.
                            <foreign>Sottoscrizioni</foreign>: + Ermitanchi viri honesti vendituri;
                        + Teufrada <rs key="#Vir_devotus/vir_honestus">viro honesta</rs>
                        venditricis; + <persName key="#Witness #Vir_devotus/vir_honestus"><name
                                ref="Auduinus">Aiduini</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs>, parente eius, testis;
                        + <persName key="#Witness #Vir_devotus/vir_honestus"><name ref="Alboinus"
                                >Albuini</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Baruncius">Baronci</name></persName> testis; +
                            <persName key="#Witness #Vir_devotus/vir_honestus"><name ref="Ursus"
                                >Ursu</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Fridualdus">Fridualdi</name></persName> parente
                        eius testis; + <persName key="#Witness #Vir_devotus/vir_honestus"><name
                                ref="Tachipertus">Tachipert</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Teudemarius">Teudimari</name></persName>
                        testis; + <persName key="#Witness #Vir_devotus/vir_honestus"><name
                                ref="Baroncellus">Baroncellu</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Troncius">Tronci</name></persName> testis; +
                            <persName key="#Witness #Vir_devotus/vir_honestus"><name ref="Mazulius"
                                >Mazulio</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Teudemarius">Teudimari</name></persName>
                        testis; + <persName key="#Witness #Vir_devotus/vir_honestus"><name
                                ref="Pincius">Pinci</name></persName>
                        <rs key="#Vir_devotus/vir_honestus">viri devoti</rs> filius quondam
                                <persName><name ref="Baruncius">Barunco</name></persName> testis. </ab>
                        </div>
