                        <rs key="#Priests">presbiter</rs> de Vennanzo <persName>
                            <name ref="Maria">Marie</name></persName> de <persName><name ref="Ildebrandus">Aldebrando</name></persName>
                        una <rs key="#Petiae">petia</rs>
